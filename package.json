{
	"name": "sniperok",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:reset": "supabase db reset",
		"db:pull": "kysely-codegen --schema sniperok --out-file ./src/lib/server/db/sniperok-schema.d.ts --include-pattern=\"sniperok.*\" --log-level info",
		"test": "vitest",
		"prod": "cross-env ORIGIN=http://localhost:3000 node server"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.17.0",
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/kit": "^2.9.0",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.6",
		"@types/eslint": "^9.6.1",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"@typescript-eslint/parser": "^8.18.2",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.66",
		"clsx": "^2.1.1",
		"cross-env": "^7.0.3",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "2.0.0-next.1",
		"globals": "^15.14.0",
		"happy-dom": "^16.0.1",
		"kysely-codegen": "^0.17.0",
		"lucide-svelte": "^0.468.0",
		"postcss": "^8.4.49",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "^5.8.1",
		"svelte-check": "^4.1.1",
		"sveltekit-flash-message": "^2.4.4",
		"sveltekit-superforms": "^2.21.1",
		"tailwind-merge": "^2.5.5",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.16",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^5.4.11",
		"vitest": "^2.1.8",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@iconify/svelte": "^4.1.0",
		"@supabase/ssr": "^0.3.0",
		"@supabase/supabase-js": "^2.47.2",
		"kysely": "^0.27.4",
		"mode-watcher": "^0.3.1",
		"pg": "^8.13.1",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"tslog": "^4.9.3",
		"zod-form-data": "^2.0.2"
	}
}
